params {
    input = 'data/data_fastq.csv'
    output_dir = 'Results'
    savescript = ''
}

process {
    withName: fastqc {
        cpus = 8
        memory = '16 GB'
        container = 'biocontainers/fastqc:v0.11.9_cv8'
    }
}

executor {
    name = 'local'
}

docker {
    enabled = true
}
